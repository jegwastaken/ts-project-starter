{
  "name": "ts-project-starter",
  "version": "0.0.1",
  "description": "A TypeScript project starter.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/jegwastaken/ts-project-starter.git",
  "author": "John Romeral",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -d",
    "start": "rimraf dist && tsc -d -w",
    "eslint:panadero": "npx install-peerdeps --dev eslint-config-panadero",
    "prettier": "prettier --write .",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "yarn prettier && yarn eslint"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-config-panadero": "0.0.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
